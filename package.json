{
    "name": "inversify-string-mapper",
    "version": "1.0.0",
    "description": "string mapper definitions",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "author": {
        "name": "Yadickson Soto",
        "email": "yadickson@gmail.com",
        "url": "https://github.com/yadickson/inversify-string-mapper"
    },
    "license": "GPL-3.0",
    "repository": {
        "url": "git+https://github.com/yadickson/inversify-string-mapper.git"
    },
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "test": "mkdirp ./reports && mocha --reporter mocha-multi-reporters --reporter-options configFile=.mocharc.json",
        "test:coverage": "nyc mocha --config .mocharc.json",
        "test:mutation": "stryker run stryker.conf.json",
        "update": "updates --update --minor",
        "clean": "rimraf ./dist ./reports",
        "build": "npm run clean && tsc -p tsconfig.build.json && tsc-alias",
        "lint": "eslint src test",
        "lint:fix": "npm run lint -- --fix",
        "prettier": "prettier src test --check",
        "prettier:fix": "npm run prettier -- --write",
        "format": "npm run prettier:fix && npm run lint:fix",
        "prepack": "npm run build"
    },
    "devDependencies": {
        "@faker-js/faker": "^8.4.1",
        "@stryker-mutator/core": "^5.6.1",
        "@stryker-mutator/mocha-runner": "^5.6.1",
        "@types/chai": "^4.3.19",
        "@types/express": "^4.17.21",
        "@types/mocha": "^9.1.1",
        "@types/node": "^17.0.45",
        "@types/rimraf": "^3.0.2",
        "@types/sanitize-html": "^2.13.0",
        "@typescript-eslint/eslint-plugin": "^5.62.0",
        "@typescript-eslint/parser": "^5.62.0",
        "chai": "^4.5.0",
        "eslint": "^8.57.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.10.0",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-import-resolver-typescript": "^2.7.1",
        "eslint-plugin-anti-trojan-source": "^1.1.1",
        "eslint-plugin-chai-expect": "^3.1.0",
        "eslint-plugin-import": "^2.30.0",
        "eslint-plugin-json": "^2.1.2",
        "eslint-plugin-mocha": "^10.5.0",
        "eslint-plugin-mocha-cleanup": "^1.11.3",
        "eslint-plugin-no-relative-import-paths": "^1.5.5",
        "eslint-plugin-no-secrets": "^0.9.1",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-promise": "^6.6.0",
        "eslint-plugin-redos": "^4.4.5",
        "eslint-plugin-security": "^1.7.1",
        "eslint-plugin-sonarjs": "^0.25.1",
        "eslint-plugin-spellcheck": "^0.0.20",
        "eslint-plugin-unicorn": "^46.0.1",
        "faker": "^6.6.6",
        "glob": "^8.1.0",
        "mkdirp": "^1.0.4",
        "mocha": "^8.4.0",
        "mocha-junit-reporter": "^2.2.1",
        "mocha-multi-reporters": "^1.5.1",
        "nyc": "^15.1.0",
        "prettier": "^2.8.8",
        "reflect-metadata": "^0.2.2",
        "rimraf": "^3.0.2",
        "source-map-support": "^0.5.21",
        "ts-node": "^10.9.2",
        "ts-sinon": "^2.0.2",
        "tsc-alias": "^1.8.10",
        "tsconfig-paths": "^4.2.0",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^4.9.5",
        "updates": "^14.5.0"
    },
    "dependencies": {
        "inversify": "^6.0.2"
    }
}
